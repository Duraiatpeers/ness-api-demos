{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": ["page", "data"],
  "properties": {
    "page": { "type": "integer" },
    "data": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "required": ["id", "email"],
        "properties": {
          "id": { "type": "integer" },
          "email": { "type": "string", "format": "email" },
          "first_name": { "type": "string" },
          "last_name": { "type": "string" },
          "avatar": { "type": "string", "format": "uri" }
        },
        "additionalProperties": true
      }
    }
  },
  "additionalProperties": true
}
